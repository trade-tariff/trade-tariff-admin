<div class="page-header">
  <h1>Edit Search Synonym</h1>
</div>

<%= simple_form_for search_reference, url: synonyms_heading_search_reference_path(heading, search_reference), as: :search_reference, html: { class: 'search-reference-form' } do |f| %>
  <%= f.input :title, label: 'Synonym' %>

  <div>
    <%= f.button :submit, class: 'btn-warning', data: { 'disable-with' => 'Working' } %>
    <%= link_to "Back", :back, class: 'btn' %>
  </div>
<% end %>
