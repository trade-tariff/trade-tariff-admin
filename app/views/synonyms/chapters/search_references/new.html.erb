<div class="page-header">
  <h1>New Search Synonym</h1>
</div>

<%= simple_form_for [:synonyms, chapter, @search_reference], html: { class: 'search-reference-form' } do |f| %>
  <%= f.input :title, label: 'Synonym' %>

  <div>
    <%= f.button :submit, class: 'btn btn-success', data: { 'disable-with' => 'Working' } %>
    <%= link_to "Back", :back, class: 'btn' %>
  </div>
<% end %>
