<div class="page-header">
  <h1>New Chapter Note</h1>
</div>

<%= simple_form_for @chapter_note, url: notes_chapter_chapter_note_path(chapter) do |f| %>
  <%= f.input 'section_title', disabled: true, input_html: { value: chapter.goods_nomenclature_item_id } %>

  <%= render 'fields', f: f %>

  <div>
    <%= f.button :submit, data: { 'disable-with' => 'Working' } %>
    <%= link_to "Back", :back, class: 'btn' %>
  </div>
<% end %>
